{
  "name": "crashify-desktop",
  "productName": "Crashify Desktop",
  "description": "Aplicación de escritorio para la revisión de reportes de accidentes de tránsito",
  "version": "1.0.0",
  "repository": "github:Manolomon/crashify-desktop",
  "license": "MIT",
  "main": "electron/dist/main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "ng build --base-href ./ && tsc --p electron && electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=dist/assets/icons/ic_launcher.icns --prune=true --out=builds",
    "package-win": "electron-packager . crashify-desktop --overwrite --asar=true --platform=win32 --arch=x64 --prune=true --out=builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Crashify\"",
    "package-linux": "electron-packager . crashify-desktop --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/mipmap-xxhdpi/ic_launcher.png --prune=true --out=builds",
    "create-installer-debian": "electron-installer-debian --src builds/crashify-desktop-linux-x64/ --arch amd64 --config electron/debian.json",
    "create-installer-win": "node electron/createinstaller.js",
    "create-installer-mac": "electron-installer-dmg ./builds/Arpa\\ Desktop-darwin-x64/Arpa\\ Desktop.app arpa-desktop-app --out=builds --overwrite --icon=assets/icons/ic_launcher.icns"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.15",
    "@angular/cdk": "^8.0.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "^8.0.0",
    "@angular/flex-layout": "^7.0.0-beta.24",
    "@angular/forms": "^7.2.15",
    "@angular/material": "^8.0.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@improbable-eng/grpc-web": "^0.9.6",
    "@types/google-protobuf": "^3.2.7",
    "angular-notifier": "^4.1.1",
    "core-js": "^2.5.4",
    "google-protobuf": "^3.8.0",
    "grpc": "^1.21.1",
    "grpc-web": "^1.0.4",
    "hammerjs": "^2.0.8",
    "js-sha256": "^0.9.0",
    "ng-image-slider": "^1.3.1",
    "ngx-toastr": "^10.0.4",
    "rxjs": "~6.3.3",
    "ts-protoc-gen": "^0.10.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.8",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/electron": "^1.6.10",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "electron": "^5.0.3",
    "electron-packager": "^13.1.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
